{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "prettier": true,
      "interface-name": false,
      "no-console": [
        "log",
        {
          "allow": [
            "error",
            "warn"
          ]
        }
      ]
    }
  }